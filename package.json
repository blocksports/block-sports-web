{
  "name": "block-sports-web",
  "version": "0.1.0",
  "author": "Mirren King-Smith",
  "private": true,
  "scripts": {
    "start": "webpack-dashboard -- node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "generate": "plop --plopfile generators/index.js",
    "toolbox": "react-toolbox-themr"
  },
  "dependencies": {
    "antd": "^2.11.2",
    "autoprefixer": "7.1.1",
    "axios": "^0.16.2",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "i18next": "^8.4.2",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-click-outside": "^3.0.0",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^16.0.0",
    "react-error-overlay": "^1.0.9",
    "react-google-maps": "^7.2.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "react-toolbox": "^2.0.0-beta.12",
    "react-tooltip": "^3.4.0",
    "react-transition-group": "^2.1.0",
    "redux": "^3.7.1",
    "redux-act": "^1.3.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "uuid": "^3.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-import": "^1.2.1",
    "babel-preset-react-app": "^3.0.1",
    "codecov": "^2.3.0",
    "css-loader": "0.28.4",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "inquirer-directory": "^2.1.0",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "plop": "^1.9.0",
    "react-test-renderer": "^16.0.0",
    "react-toolbox-themr": "^1.0.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "style-loader": "0.18.2",
    "webpack": "2.6.1",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0"
  },
  "peerDependencies": {
    "less": "^2.7.2"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "globals": {
      "window": true
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTestFramework.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "pusher": true
    }
  },
  "reactToolbox": {
    "include": [
      "SLIDER"
    ],
    "customProperties": {
      "animation-duration": "0.3s",
      "color-accent": "var(--palette-pink-a200)",
      "color-accent-dark": "var(--palette-pink-700)",
      "color-primary-contrast": "var(--color-dark-contrast)",
      "color-accent-contrast": "var(--color-dark-contrast)"
    },
    "output": "assets/react-toolbox"
  }
}
